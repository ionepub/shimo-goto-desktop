{
	"name": "Go to desktop",
	"version": "1.0",
	"manifest_version": 2,
	"description": "打开石墨文档(shimo.im)的首页时，让页面自动跳转到石墨桌面",
	"browser_action": {
		"default_icon": "image/icon.png"
	},
	"icons": {
		"48": "image/icon48.png",
		"128": "image/icon128.png"
    },
	"content_scripts": [
		{
		  "matches": ["https://shimo.im/"],
		  "js": ["app.js"]
		}
	]

}